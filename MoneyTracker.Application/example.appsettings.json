{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IsPostgreInUse": true,
  "ConnectionStrings": {
    // Use for localhost database instance
    "MsSqlConnection": "Server=localhost;Database=MoneyTracker;Trusted_Connection=True;Connect Timeout=15;Encrypt=False;Packet Size=4096",
    "PostgreSqlConnection": "<YourConnectionString>"
  },
  // JWT Token Configuration
  "JwtConfiguration": {
    "ValidIssuer": "https://localhost:7003/",
    "ValidAudience": "https://localhost:7003/",
    "Secret": "yourSuperStrongSecret",
    "ExpiresIn": 60
  },
  // CORS Configuration
  "JsCors": "http://localhost:7003"
}